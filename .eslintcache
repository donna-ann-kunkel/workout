al}>\n        <div>\n          <div>{cartItems}</div>\n        </div>\n\n        <button className={styles.button} onClick={props.onClose}>\n          Close\n        </button>\n        <button className={styles.button} onClick={submitWorkoutHandler}>\n          Complete Workout\n        </button>\n      </div>\n    </Fragment>\n  );\n};\nexport default Modal;\n","/home/marcus/Documents/donna_portfolio/workout_donna_react/builder_buddy/src/components/WorkoutHistory.js",[],"/home/marcus/Documents/donna_portfolio/workout_donna_react/builder_buddy/src/components/CartItem.js",[],"/home/marcus/Documents/donna_portfolio/workout_donna_react/builder_buddy/src/components/WorkoutItem.js",[],"/home/marcus/Documents/donna_portfolio/workout_donna_react/builder_buddy/src/components/HistoryItem.js",[],"/home/marcus/Documents/donna_portfolio/workout_donna_react/builder_buddy/src/components/Search.js",[],"/home/marcus/Documents/donna_portfolio/workout_donna_react/builder_buddy/src/components/DateFilter.js",["115","116"],{"ruleId":"117","severity":1,"message":"118","line":5,"column":8,"nodeType":"119","messageId":"120","endLine":5,"endColumn":19},{"ruleId":"117","severity":1,"message":"121","line":5,"column":23,"nodeType":"119","messageId":"120","endLine":5,"endColumn":42},{"ruleId":"117","severity":1,"message":"122","line":31,"column":9,"nodeType":"119","messageId":"120","endLine":31,"endColumn":31},{"ruleId":"117","severity":1,"message":"123","line":5,"column":10,"nodeType":"119","messageId":"120","endLine":5,"endColumn":18},{"ruleId":"117","severity":1,"message":"124","line":9,"column":9,"nodeType":"119","messageId":"120","endLine":9,"endColumn":22},{"ruleId":"117","severity":1,"message":"125","line":11,"column":10,"nodeType":"119","messageId":"120","endLine":11,"endColumn":19},{"ruleId":"117","severity":1,"message":"118","line":5,"column":8,"nodeType":"119","messageId":"120","endLine":5,"endColumn":19},{"ruleId":"117","severity":1,"message":"121","line":5,"column":23,"nodeType":"1